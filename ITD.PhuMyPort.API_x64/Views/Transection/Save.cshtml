
@{
    ViewData["Title"] = "Save";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-12">
        <form asp-action="Save" asp-controller="Transection" method="post" enctype="multipart/form-data">
            <div class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">File Csv</label>
                <input type="file" name="file" enctype='multipart/form-data' class="form-control" />

            </div>
            <div class="form-group">
                <label class="control-label">File Images</label>
                <input type="file" name="photos" multiple="multiple" enctype='multipart/form-data' class="form-control" />

            </div>
           

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

